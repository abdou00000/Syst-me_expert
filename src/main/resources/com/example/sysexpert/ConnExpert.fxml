<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="343.0" prefWidth="339.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sysexpert.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <TextField fx:id="Pseudo" promptText="Pseudo" />
    <TextField fx:id="PassWord" promptText="Mot de passe" />

    <Label fx:id="WelcomeText" />
    <Button fx:id="LogIn" onAction="#onLogIn" text="Se Connecter" />
   <Button mnemonicParsing="false" onAction="#OnRetour" text="Retour" />
</VBox>
